{
  "name": "FaceSure",
  "version": "0.0.4",
  "description": "face detection app for attendance of employees",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "publish": "build -p always"
  },
  "author": "prajwal kamble",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.466.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "fs-extra": "^8.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^20.41.0"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "prajwal1999",
        "repo": "face-attendance"
      }
    ],
    "appId": "com.augleAI.facex",
    "productName": "FaceX",
    "target": "NSIS",
    "directories": {
      "output": "out"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  }
}
